        var $this = $(this);
        if ($this.val() == brand) {
           $this.attr('selected','selected');
           return false;
        }
      });

      var categ = $(this).data('prodcateg');
      $('#view_pcat').find('option').each( function() {
        var $this = $(this);
        if ($this.val() == categ) {
           $this.attr('selected','selected');
           return false;
        }
      });

    });
  </script>


