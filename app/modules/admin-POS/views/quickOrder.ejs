<%- include('../../../templates/admin-navbar.ejs') -%>

<!-- Content Wrapper. Contains page content -->

<div class="content-wrapper">
   <!-- Content Header (Page header) -->
   <section class="content-header">
     <h1>

       <small>UNDER CONSTRUCTION</small>
     </h1>
     <ol class="breadcrumb">
       <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
       <li><a href="#">Orders</a></li>
       <li class="active">Orders</li>
     </ol>
   </section>

   <!-- Main content -->
   <section class="content">

    <div class="row">
      <div class="col-sm-8">

        <div class="row">
          <div class="col-sm-12">
            <div class="box box-info">
              <div class="box-header with-border">
                <h3 class="box-title"></h3>
              </div>
              <div class="box-body">
                <div class="input-group">
                  <div class="input-group-btn">
                    <button id="execute" type="button" class="btn btn-info"><i class="fa fa-plus"></i> </button>
                  </div>
                  <!-- /btn-group -->
                  <input type="text" placeholder="Enter product name or barcode" class="form-control">

                  <div class="input-group-btn">
                    <a href="/sales/try"><button  type="button" class="btn btn-success"><i class="fa fa-search"></i> Search</button></a>
                  </div>

                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- End of row -->

        <div class="row">
          <div class="col-sm-12">
            <div class="box box-solid">
              <div class="box-header with-border">
              </div>
              <div id="orderList" class="box-body table-responsive">
                <table class="table table-bordered table-hover">
                  <thead>
                    <tr>
                      <th style="width: 10px">No</th>
                      <th>Product Name</th>
                      <th>Quantity</th>
                      <th>Price</th>
                      <th>SubTotal</th>
                      <th>Action</th>

                    </tr>
                  </thead>
                </table>
              </div>
            </div>
          </div>
        </div>
        <!-- End of row -->
      </div>
      <!-- End of Left Column -->

      <div class="col-sm-4">
        <div class="row">
          <div class="col-sm-12">
            <div class="box box-solid">

              <div class="box-body">
                <div class="input-group">
                  <div class="input-group-btn">
                    <button type="button" class="btn btn-info"><i class="fa fa-plus"></i> </button>
                  </div>
                  <!-- /btn-group -->
                  <input type="text" placeholder="Enter customer name" class="form-control">


                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- End of row -->

        <div class="row">
          <div class="col-sm-12">
            <div class="box box-solid">
              <div class="box-body">
                <p style="">Discount: </p>
                <a href="#">Set Discount</a>

                <label>Total:</label>
                <p></p>
              </div>
            </div>
          </div>
        </div>



      </div>
      <!-- End of Right Column -->
    </div>
    <!-- End of row -->





   </section>
   <!-- /.content -->
</div>

 <!-- /.content-wrapper -->

