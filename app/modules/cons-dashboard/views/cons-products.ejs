<%- include('../../../templates/consignor-dashboard.ejs') -%>

    <div id="consigner" class="container">
		<div class="row">
		  <div class="col">
			<div id="main-content">
				<div class="col-lg-12">
					<div class="card-title">
						<h4>  Products </h4>
					</div>
					<div class="card-body">
						<div class="table-responsive">
							<table class="table">
								<thead>
									<tr>
										<th>&nbsp</th>
										<th>Product Name</th>
										<th>Store Price</th>
										<th>Remaining Stock</th>
										<th>Category</th>


									</tr>
								</thead>
								<tbody>
									<% if (re == null || re == undefined){ %>

										<% }else if (re.length == 0){ %>

										<% }else{ %>
											<%  re.forEach(function(i){ %>
												<tr>
													<td>

														<div class="round-img" style="text-align: center;">
															<a href=""><img src="/assets/images/products/<%=i.strProductPicture%>"></a>
														</div>
													</td>
													<td><%= i.strProductName%></td>
													<td><span><%= i.productPrice%></span></td>
													<td><span><%= i.intQuantity%></span></td>
													<td><span class="badge badge-default"><%= i.strCategory%></span></td>
												
												</tr>
											<%  }) %>
											<% } %>
								</tbody>
							</table>
						</div>
					</div>

					</div>
				</div>
				</div>
		</div>
		</div>

    <script src="/assets/consignor/jquery-slim.js"></script>
    <script src="/assets/consignor/popper.min.js"></script>
    <script src="/assets/consignor/bootstrap.min.js"></script>
    <script src="/assets/consignor/simplebar.js"></script>

    <script src="/assets/admin/bower_components/jquery/dist/jquery.min.js"></script>

    <script src="/assets/admin/bower_components/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="/assets/admin/bower_components/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>

  </body>

  </html>
