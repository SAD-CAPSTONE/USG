<%- include('../../../templates/admin-navbar.ejs') -%>

<!-- Content Wrapper. Contains page content -->

<div class="content-wrapper">
   <!-- Content Header (Page header) -->
   <section class="content-header">
     <h1>

       <small></small>
     </h1>
     <ol class="breadcrumb">
       <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
       <li><a href="#">Consignment Agreement</a></li>
       <li class="active">Contract History</li>
     </ol>
   </section>

   <!-- Main content -->
   <section class="content">
     <div class="row">
       <div class="col-xs-12">
         <div class="box">
           <div class="box-header">
             <h4 align="center">Contract History</h3>
           </div>
    <!-- /.box-header -->
           <div class="box-body table-responsive">
             <table id="example2" class="table table-bordered table-hover">
               <thead>
               <tr>
                 <th>No</th>
                 <th>Date</th>
                 <th>Status</th>

               </tr>
               </thead>
               <tbody>
                <% if (re == null||re==undefined){ %>

                <% }else if(re.length == 0){ %>

                <% }else{ %>
                  <% re.forEach(function(i){ %>
                    <tr>
                      <td><%=i.intContractHistoryNo%></td>
                      <td><%=moment(i.historyDate).format("MM/DD/YYYY")%></td>
                      <td>
                        <% if(i.intContractStatus == 4){ %>
                          <span class="label label-danger">Terminated</span>

                        <% }else if(i.intContractStatus == 1){ %>
                          <span class="label label-success">Accepted</span>

                        <% }else if(i.intContractStatus == 0){ %>
                          <span class="label label-warning">Pending</span>

                        <% }else{ %>
                          <span>No label</span>

                        <% } %>
                      </td>

                    </tr>
                  <% }) %>
                <% } %>

               </tbody>

             </table>
           </div>
           <!-- /.box-body -->
         </div>
         <!-- /.box -->

     </div>
       <!-- /.col -->
     </div>
     <!-- /.row -->
   </section>
   <!-- /.content -->
</div>

 <!-- /.content-wrapper -->

 <!-- Modals -->

 <%- include('../../../templates/admin-aside.ejs') -%>

<!-- ./wrapper -->

