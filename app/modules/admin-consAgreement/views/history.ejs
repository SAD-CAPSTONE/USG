 $(function () {
   $('#example2').DataTable({
     'paging'      : true,
     'lengthChange': true,
     'searching'   : true,
     'ordering'    : true,
     'info'        : true,
     'autoWidth'   : true,
   'select'		: true,
   'scroll'		: true
   })
 })
</script>

<script>
$('#title').html("USG | Batch");
$(".treeview a:contains('Transactions')").parent().addClass("active");
$(".navi a:contains('Consignment Agreement')").parent().addClass("active");
</script>


