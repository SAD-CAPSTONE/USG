<div class="table-responsive">

  <table id="transactions" class="table no-margin">
    <thead>
      <tr>
        <th>Date</th>
        <th>Product Name</th>
        <th>Transaction</th>

      </tr>
    </thead>

    <tbody>
      <% if(re==null|re==undefined){ %>

      <% }else if(re.length==0){ %>

      <% }else{ %>
      <%  re.forEach(function(i){ %>
        <tr>
          <td><%=moment(i.transactionDate).format("MM/DD/YYYY HH:mm")%></td>
          <td><%=i.strProductName%> <%=i.strVariant%> <%=i.intSize%> <%=i.strUnitName%></td>
          <td><%=i.strTypeOfChanges%></td>
        </tr>
      <%  }) %>
      <% } %>

    </tbody>
  </table>
</div>

<script>
$(function () {
  $('#transactions').DataTable({
    'paging'      : false,
    'lengthChange': false,
    'searching'   : true,
    'ordering'    : true,
    'info'        : true,
    'autoWidth'   : true,
  'select'		: true,
  'scroll'		: true
  })
})
</script>
